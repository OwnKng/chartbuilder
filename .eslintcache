eWarningCount":0,"usedDeprecatedRules":"128"},{"filePath":"247","messages":"248","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"128"},{"filePath":"249","messages":"250","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},{"filePath":"251","messages":"252","errorCount":0,"warningCount":1,"fixableErrorCount":0,"fixableWarningCount":0,"source":null},{"filePath":"253","messages":"254","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"128"},"/Users/owenking/web-development/data-viz-app/src/index.js",[],["255","256"],"/Users/owenking/web-development/data-viz-app/src/reportWebVitals.js",[],"/Users/owenking/web-development/data-viz-app/src/data/index.js",[],"/Users/owenking/web-development/data-viz-app/src/data/lifeExpectancy.js",[],"/Users/owenking/web-development/data-viz-app/src/hooks/selectionProvider.js",["257"],"/Users/owenking/web-development/data-viz-app/src/hooks/SelectionProvider.js",[],"/Users/owenking/web-development/data-viz-app/src/hooks/index.js",[],"/Users/owenking/web-development/data-viz-app/src/hooks/useSelection.js",[],"/Users/owenking/web-development/data-viz-app/src/hooks/useScale.js",[],"/Users/owenking/web-development/data-viz-app/src/hooks/useType.js",[],"/Users/owenking/web-development/data-viz-app/src/components/visualisations/Scatter.js",[],"/Users/owenking/web-development/data-viz-app/src/components/visualisations/Bar.js",["258","259","260"],"import ParentSize from \"@visx/responsive/lib/components/ParentSize\"\nimport { Group } from \"@visx/group\"\nimport { Bar } from \"@visx/shape\"\nimport { format, bisectRight } from \"d3\"\nimport { useSelection } from \"../../hooks\"\nimport { scaleBand, scaleLinear, scaleOrdinal } from \"@visx/scale\"\nimport { max } from \"d3\"\nimport AxisLeft from \"./AxisLeft\"\nimport AxisBottom from \"./AxisBottom\"\nimport Legend from \"./Legend\"\nimport { AnimatedGridRows } from \"@visx/react-spring\"\nimport { useCallback } from \"react\"\nimport { useTooltip, TooltipWithBounds } from \"@visx/tooltip\"\nimport { localPoint } from \"@visx/event\"\nimport { palettes } from \"../styled/utilities\"\n\nconst BarChart = ({\n  width,\n  height,\n  margin = { top: 60, left: 60, right: 30, bottom: 80 },\n}) => {\n  const { data, x, y, color, reordered, palette } = useSelection()\n\n  let barData = [...data]\n\n  if (reordered) barData.sort((a, b) => b[y] - a[y])\n\n  // Set dimensions\n  const innerWidth = width - margin.left - margin.right\n  const innerHeight = height - margin.bottom - margin.top\n\n  // Accessor functions\n  const getX = (d) => d[x]\n  const getY = (d) => d[y] ?? 0\n  const getColor = (d) => d[color]\n\n  // Create scales\n  const xScale = scaleBand({\n    domain: [...new Set(barData.map(getX))],\n    range: [margin.left, innerWidth + margin.left],\n    padding: 0.1,\n  })\n\n  const yScale = scaleLinear({\n    domain: [0, max(barData, getY)],\n    range: [innerHeight + margin.top, margin.top],\n    nice: true,\n  })\n\n  const colorScale = scaleOrdinal({\n    domain: [...new Set(barData.map(getColor))],\n    range: palettes[palette],\n  })\n\n  const {\n    showTooltip,\n    hideTooltip,\n    tooltipData,\n    tooltipTop = 0,\n    tooltipLeft = 0,\n  } = useTooltip()\n\n  const handleTooltip = useCallback(\n    (event) => {\n      const { x: xPos } = localPoint(event)\n      const range = xScale.domain().map(xScale)\n      const index = bisectRight(range, xPos)\n      const xKeys = barData.map((d) => getX(d))\n\n      const selected = barData.filter((d) => getX(d) === xKeys[index - 1])\n      const yMax = max(selected, getY)\n\n      showTooltip({\n        tooltipData: selected,\n        tooltipLeft: xScale(xKeys[index - 1]),\n        tooltipTop: yScale(yMax),\n      })\n    },\n    [barData, getX, getY, showTooltip, xScale, yScale]\n  )\n\n  // Return the chart\n  return (\n    <>\n      <svg width={width} height={height}>\n        <g transform={`translate(${margin.left},0)`}>\n          <AxisLeft scale={yScale} />\n          <AnimatedGridRows\n            scale={yScale}\n            stroke={\"var(--color-paragraph)\"}\n            strokeWidth={0.2}\n            width={innerWidth}\n          />\n        </g>\n        <Group>\n          {barData.map((d, i) => {\n            const barWidth = xScale.bandwidth()\n            const barHeight = innerHeight + margin.top - yScale(getY(d))\n            const barX = xScale(getX(d))\n            const barY = innerHeight + margin.top - barHeight\n            return (\n              <Bar\n                key={`bar-${i}`}\n                x={barX}\n                y={barY}\n                width={barWidth}\n                height={barHeight}\n                fill={colorScale(getColor(d))}\n              />\n            )\n          })}\n        </Group>\n        <AxisBottom\n          top={innerHeight + margin.top}\n          animated={false}\n          scale={xScale}\n        />\n        <rect\n          x={margin.left}\n          y={margin.top}\n          width={innerWidth}\n          height={innerHeight}\n          fill='transparent'\n          onTouchStart={handleTooltip}\n          onTouchMove={handleTooltip}\n          onMouseMove={handleTooltip}\n          onMouseLeave={() => hideTooltip()}\n        />\n      </svg>\n      {tooltipData && (\n        <TooltipWithBounds\n          key={Math.random()}\n          top={tooltipTop - 40}\n          left={tooltipLeft}\n        >\n          {tooltipData.map((row) => (\n            <div>\n              <p>{getX(row)}</p>\n              <span>{format(\",d\")(getY(row))}</span>\n            </div>\n          ))}\n        </TooltipWithBounds>\n      )}\n      {color !== \"none\" ? (\n        <Legend left={margin.left} scale={colorScale} />\n      ) : null}\n    </>\n  )\n}\n\nconst ChartWrapper = () => (\n  <ParentSize>\n    {({ width, height }) => <BarChart width={width} height={height} />}\n  </ParentSize>\n)\n\nexport default ChartWrapper\n","/Users/owenking/web-development/data-viz-app/src/components/visualisations/Line.js",[],"/Users/owenking/web-development/data-viz-app/src/components/visualisations/visOptions.js",[],"/Users/owenking/web-development/data-viz-app/src/client.js",[],"/Users/owenking/web-development/data-viz-app/src/components/pages/index.js",[],"/Users/owenking/web-development/data-viz-app/src/components/pages/home.js",[],"/Users/owenking/web-development/data-viz-app/src/components/styled/elements/Control.js",[],"/Users/owenking/web-development/data-viz-app/src/components/styled/elements/ButtonOptions.js",[],"/Users/owenking/web-development/data-viz-app/src/components/App.js",[],"/Users/owenking/web-development/data-viz-app/src/components/GlobalStyles.js",[],"/Users/owenking/web-development/data-viz-app/src/components/styled/layout/Header.js",[],"/Users/owenking/web-development/data-viz-app/src/components/styled/utilities/index.js",[],"/Users/owenking/web-development/data-viz-app/src/components/styled/utilities/Elevation.js",[],"/Users/owenking/web-development/data-viz-app/src/components/styled/elements/Panel.js",[],"/Users/owenking/web-development/data-viz-app/src/components/styled/layout/Footer.js",[],"/Users/owenking/web-development/data-viz-app/src/components/visualisations/AxisLeft.js",[],"/Users/owenking/web-development/data-viz-app/src/components/visualisations/AxisBottom.js",[],"/Users/owenking/web-development/data-viz-app/src/components/visualisations/Legend.js",[],"/Users/owenking/web-development/data-viz-app/src/components/styled/layout/Layout.js",[],"/Users/owenking/web-development/data-viz-app/src/hooks/useActive.js",[],"/Users/owenking/web-development/data-viz-app/src/components/styled/elements/Button.js",[],"/Users/owenking/web-development/data-viz-app/src/components/styled/elements/Select.js",[],"/Users/owenking/web-development/data-viz-app/src/components/styled/elements/NextSpan.js",[],["261","262"],"/Users/owenking/web-development/da