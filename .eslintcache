parse(event.target.value, {\n      header: true,\n      dynamicTyping: true,\n    })\n\n    if (errors.length) {\n      setData(false)\n      return null\n    }\n    setData(data)\n  }\n\n  return (\n    <Modal maxWidth={800}>\n      <CloseButton onClick={() => toggle()}>X</CloseButton>\n      <div className={className}>\n        <h4>Add data</h4>\n        <div className='flex'>\n          <div className='input'>\n            <form>\n              <label className='textLabel' htmlFor='dataName'>\n                Name of dataset\n              </label>\n              <input\n                className='nameInput'\n                id='dataName'\n                name='name'\n                value={title}\n                onChange={(event) => setTitle(event.target.value)}\n              />\n              <label className='textLabel' htmlFor='dataInput'>\n                Paste csv data here\n              </label>\n              <textarea id='dataInput' name='data' onChange={handleCSV} />\n            </form>\n          </div>\n          <div className='preview'>\n            <p>preview</p>\n            {data ? <DataTable data={data} /> : <span>Awaiting Valid CSV</span>}\n          </div>\n        </div>\n        <Button\n          type='submit'\n          onClick={() => {\n            if (data && title) {\n              createDataset({\n                variables: {\n                  data: JSON.stringify(data),\n                  name: title,\n                },\n              })\n            }\n          }}\n        >\n          Add new data\n        </Button>\n      </div>\n    </Modal>\n  )\n}\n\nexport default styled(DataInput)`\n  padding: 1rem;\n\n  h4 {\n    text-align: center;\n    margin-bottom: 10px;\n  }\n\n  .flex {\n    display: flex;\n\n    @media only screen and (max-width: 600px) {\n      flex-direction: column;\n    }\n  }\n\n  .input {\n    flex-grow: 1;\n  }\n\n  form {\n    color: var(--color-paragraph);\n    display: flex;\n    flex-direction: column