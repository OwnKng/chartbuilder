.js",[],"/Users/owenking/web-development/data-viz-app/src/data/index.js",[],"/Users/owenking/web-development/data-viz-app/src/data/lifeExpectancy.js",[],"/Users/owenking/web-development/data-viz-app/src/hooks/selectionProvider.js",["277"],"/Users/owenking/web-development/data-viz-app/src/hooks/SelectionProvider.js",[],"/Users/owenking/web-development/data-viz-app/src/hooks/index.js",[],"/Users/owenking/web-development/data-viz-app/src/hooks/useSelection.js",[],"/Users/owenking/web-development/data-viz-app/src/hooks/useScale.js",[],"/Users/owenking/web-development/data-viz-app/src/hooks/useType.js",[],"/Users/owenking/web-development/data-viz-app/src/components/visualisations/Scatter.js",["278","279","280","281","282"],"import ParentSize from \"@visx/responsive/lib/components/ParentSize\"\nimport { Circle } from \"@visx/shape\"\nimport { scaleOrdinal } from \"@visx/scale\"\nimport { AnimatedGridRows } from \"@visx/react-spring\"\nimport { useSelection } from \"../../hooks\"\nimport { useScale } from \"../../hooks\"\nimport AxisLeft from \"./AxisLeft\"\nimport AxisBottom from \"./AxisBottom\"\nimport { scaleLinear } from \"@visx/scale\"\nimport { extent } from \"d3\"\nimport { palettes } from \"../styled/utilities\"\nimport Legend from \"./Legend\"\nimport { useTooltip, TooltipWithBounds, defaultStyles } from \"@visx/tooltip\"\nimport { localPoint } from \"@visx/event\"\nimport { voronoi } from \"@visx/voronoi\"\nimport { useRef, useMemo, useCallback } from \"react\"\n\nconst Chart = ({\n  width,\n  height,\n  margin = { top: 60, left: 60, right: 30, bottom: 80 },\n}) => {\n  const { data, x, y, label, color, geometry, palette } = useSelection()\n\n  // Set dimensions\n  const innerWidth = width - margin.left - margin.right\n  const innerHeight = height - margin.bottom - margin.top\n\n  // Accessor functions\n  const getX = (d) => d[x]\n  const getY = (d) => d[y]\n  const getLabel = (d) => d[label]\n  const getColor = (d) => d[color]\n\n  // Create scales\n  const [xScale] = useScale(data, getX)\n  xScale.range([margin.left, innerWidth + margin.left])\n\n  const yScale = scaleLinear({\n    range: [innerHeight + margin.top, margin.top],\n    domain: extent(data, getY),\n    nice: true,\n  })\n\n  const colorScale = scaleOrdinal({\n    domain: [...new Set(data.map(getColor))],\n    range: palettes[palette],\n  })\n\n  // Handle the tooltip\n  const {\n    showTooltip,\n    hideTooltip,\n    tooltipData,\n    tooltipOpen,\n    tooltipTop = 0,\n    tooltipLeft = 0,\n  } = useTooltip()\n\n  const svgRef = useRef(null)\n\n  const voronoiLayout = useMemo(\n    () =>\n      voronoi({\n        x: (d) => xScale(getX(d)) ?? 0,\n        y: (d) => yScale(getY(d)) ?? 0,\n        width,\n        height,\n      })(data),\n    [data, width, height, getX, getY, xScale, yScale]\n  )\n\n  let tooltipTimeout\n  const handleMouseMove = useCallback(\n    (event) => {\n      if (tooltipTimeout) clearTimeout(tooltipTimeout)\n      if (!svgRef.current) return\n      const point = localPoint(svgRef.current, event)\n      if (!point) return\n      const neighborRadius = 100\n      const closest = voronoiLayout.find(point.x, point.y, neighborRadius)\n      if (closest) {\n        showTooltip({\n          tooltipLeft: xScale(getX(closest.data)),\n          tooltipTop: yScale(getY(closest.data)),\n          tooltipData: closest.data,\n        })\n      }\n    },\n    [getX, getY, xScale, yScale, showTooltip, voronoiLayout, tooltipTimeout]\n  )\n\n  const handleMouseLeave = useCallback(() => {\n    tooltipTimeout = window.setTimeout(() => {\n      hideTooltip()\n    }, 1500)\n  }, [hideTooltip])\n\n  // Return the chart\n  return (\n    <>\n      <svg width={width} height={height} ref={svgRef}>\n        <rect\n          x={margin.left}\n          y={margin.top}\n          width={innerWidth}\n          height={innerHeight}\n          fill='transparent'\n          onMouseMove={handleMouseMove}\n          onMouseLeave={handleMouseLeave}\n          onTouchMove={handleMouseMove}\n          onTouchEnd={handleMouseLeave}\n        />\n        <g transform={`translate(${margin.left},0)`}>\n          <AxisLeft scale={yScale} />\n          <AnimatedGridRows\n            scale={yScale}\n            stroke={\"var(--color-paragraph)\"}\n            strokeWidth={0.2}\n            width={innerWidth}\n          />\n        </g>\n        <AxisBottom top={innerHeight + margin.top} scale={xScale} />\n        {geometry === \"point\" &&\n          data.map((point, i) => (\n            <Circle\n              key={`point-${i}`}\n              cx={xScale(getX(point))}\n              cy={yScale(getY(point))}\n              r={4}\n              fill={colorScale(getColor(point))}\n            />\n          ))}\n      </svg>\n      {label !== \"none\" &&\n        tooltipOpen &&\n        tooltipData &&\n        tooltipLeft != null &&\n        tooltipTop != null && (\n          <TooltipWithBounds\n            left={tooltipLeft + 10}\n            top={tooltipTop + 10}\n            style={defaultStyles}\n          >\n            <h3\n              style={{\n                color: colorScale(getColor(tooltipData)),\n                paddding: 0,\n                margin: 0,\n              }}\n            >\n              {getLabel(tooltipData)}\n            </h3>\n          </TooltipWithBounds>\n        )}\n      {color !== \"none\" ? (\n        <Legend left={margin.left} scale={colorScale} />\n      ) : null}\n    </>\n  )\n}\n\nconst ChartWrapper = () => (\n  <ParentSize>\n    {({ width, height }) => <Chart width={width} height={height} />}\n  </ParentSize>\n)\n\nexport default ChartWrapper\n","/Users/owenking/web-development/data-viz-app/src/components/visualisations/Bar.js",["283","284","285"],"import ParentSize from \"@visx/responsive/lib/components/ParentSize\"\nimport { Group } from \"@visx/group\"\nimport { Bar } from \"@visx/shape\"\nimport { format, bisectRight } from \"d3\"\nimport { useSelection } from \"../../hooks\"\nimport { scaleBand, scaleLinear, scaleOrdinal } from \"@visx/scale\"\nimport { max } from \"d3\"\nimport AxisLeft from \"./AxisLeft\"\nimport AxisBottom from \"./AxisBottom\"\nimport Legend from \"./Legend\"\nimport { AnimatedGridRows } from \"@visx/react-spring\"\nimport { useCallback } from \"react\"\nimport { useTooltip, TooltipWithBounds } from \"@visx/tooltip\"\nimport { localPoint } from \"@visx/event\"\nimport { palettes } from \"../styled/utilities\"\n\nconst BarChart = ({\n  width,\n  height,\n  margin = { top: 60, left: 60, right: 30, bottom: 80 },\n}) => {\n  const { data, x, y, color, reordered, palette } = useSelection()\n\n  let barData = [...data]\n\n  if (reordered) barData.sort((a, b) => b[y] - a[y])\n\n  // Set dimensions\n  const innerWidth = width - margin.left - margin.right\n  const innerHeight = height - margin.bottom - margin.top\n\n  // Accessor functions\n  const getX = (d) => d[x]\n  const getY = (d) => d[y] ?? 0\n  const getColor = (d) => d[color]\n\n  // Create scales\n  const xScale = scaleBand({\n    domain: [...new Set(barData.map(getX))],\n    range: [margin.left, innerWidth + margin.left],\n    padding: 0.1,\n  })\n\n  const yScale = scaleLinear({\n    domain: [0, max(barData, getY)],\n    range: [innerHeight + margin.top, margin.top],\n    nice: true,\n  })\n\n  const colorScale = scaleOrdinal({\n    domain: [...new Set(barData.map(getColor))],\n    range: palettes[palette],\n  })\n\n  const {\n    showTooltip,\n    hideTooltip,\n    tooltipData,\n    tooltipTop = 0,\n    tooltipLeft = 0,\n  } = useTooltip()\n\n  const handleTooltip = useCallback(\n    (event) => {\n      const { x: xPos } = localPoint(event)\n      const range = xScale.domain().map(xScale)\n      const index = bisectRight(range, xPos)\n      const xKeys = barData.map((d) => getX(d))\n\n      const selected = barData.filter((d) => getX(d) === xKeys[index - 1])\n      const yMax = max(selected, getY)\n\n      showTooltip({\n        tooltipData: selected,\n        tooltipLeft: xScale(xKeys[index - 1]),\n        tooltipTop: yScale(yMax),\n      })\n    },\n    [barData, getX, getY, showTooltip, xScale, yScale]\n  )\n\n  // Return the chart\n  return (\n    <>\n      <svg width={width} height={height}>\n        <g transform={`translate(${margin.left},0)`}>\n          <AxisLeft scale={yScale} />\n          <AnimatedGridRows\n            scale={yScale}\n            stroke={\"var(--color-paragraph)\"}\n            strokeWidth={0.2}\n            width={innerWidth}\n          />\n        </g>\n        <Group>\n          {barData.map((d, i) => {\n            const barWidth = xScale.bandwidth()\n            const barHeight = innerHeight + margin.top - yScale(getY(d))\n            const barX = xScale(getX(d))\n            const barY = innerHeight + margin.top - barHeight\n            return (\n              <Bar\n                key={`bar-${i}`}\n                x={barX}\n                y={barY}\n                width={barWidth}\n                height={barHeight}\n                fill={colorScale(getColor(d))}\n              />\n            )\n          })}\n        </Group>\n        <AxisBottom\n          top={innerHeight + margin.top}\n          animated={false}\n          scale={xScale}\n        />\n        <rect\n          x={margin.left}\n          y={margin.top}\n          width={innerWidth}\n          height={innerHeight}\n          fill='transparent'\n          onTouchStart={handleTooltip}\n          onTouchMove={handleTooltip}\n          onMouseMove={handleTooltip}\n          onMouseLeave={() => hideTooltip()}\n        />\n      </svg>\n      {tooltipData && (\n        <TooltipWithBounds\n          key={Math.random()}\n          top={tooltipTop - 40}\n          left={tooltipLeft}\n        >\n          {tooltipData.map((row) => (\n            <div>\n              <p>{getX(row)}</p>\n              <span>{format(\",d\")(getY(row))}</span>\n            </div>\n          ))}\n        </TooltipWithBounds>\n      )}\n      {color !== \"none\" ? (\n        <Legend left={margin.left} scale={colorScale} />\n      ) : null}\n    </>\n  )\n}\n\nconst ChartWrapper = () => (\n  <ParentSize>\n    {({ width, height }) => <BarChart width={width} height={height} />}\n  </ParentSize>\n)\n\nexport default ChartWrapper\n","/Users/owenking/web-development/data-viz-app/src/components/visualisations/Line.js",["286"],"import ParentSize from \"@visx/responsive/lib/components/ParentSize\"\nimport { useSelection } from \"../../hooks\"\nimport { scaleLinear, scaleOrdinal } from \"@visx/scale\"\nimport { extent, group, max, min } from \"d3\"\nimport { LinePath, Bar, Line } from \"@visx/shape\"\nimport Legend from \"./Legend\"\nimport AxisLeft from \"./AxisLeft\"\nimport AxisBottom from \"./AxisBottom\"\nimport { AnimatedGridRows } from \"@visx/react-spring\"\nimport { palettes } from \"../styled/utilities\"\nimport { useTooltip, TooltipWithBounds, defaultStyles } from \"@visx/tooltip\"\nimport { localPoint } from \"@visx/event\"\nimport { useCallback } from \"react\"\nimport styled from \"styled-components\"\n\nconst tooltipStyles = {\n  ...defaultStyles,\n  zIndex: 1,\n}\n\nconst LineChart = ({\n  className,\n  width,\n  height,\n  margin = { top: 60, left: 60, right: 30, bottom: 80 },\n}) => {\n  // dimensions\n  const { data, x, y, color, palette } = useSelection()\n\n  // Set dimensions\n  const innerWidth = width - margin.left - margin.right\n  const innerHeight = height - margin.bottom - margin.top\n\n  // Accessor functions\n  const getX = (d) => d[x]\n  const getY = (d) => d[y]\n  const getColor = (d) => d[color]\n\n  // Create scales\n  const xScale = scaleLinear({\n    domain: extent(data, getX),\n    range: [margin.left, innerWidth + margin.left],\n    clamp: true,\n  })\n\n  const yScale = scaleLinear({\n    domain: extent(data, getY),\n    range: [innerHeight + margin.top, margin.top],\n    nice: true,\n  })\n\n  const colorScale = scaleOrdinal({\n    domain: [...new Set(data.map(getColor))],\n    range: palettes[palette],\n  })\n\n  // Group data\n  const dataGrouped = Array.from(\n    group(data, (d) => d[color]),\n    ([key, value]) => ({ key, value })\n  )\n\n  // Handle the tooltip\n  const {\n    showTooltip,\n    hideTooltip,\n    tooltipData,\n    tooltipTop = 0,\n    tooltipLeft = 0,\n  } = useTooltip()\n\n  // event handler\n  const handleTooltip = useCallback(\n    (event) => {\n      const { x } = localPoint(event)\n      let x0 = xScale.invert(x)\n\n      // for mobile, prevents tooltip from crashing the app\n      if (x0 > max(data, getX)) x0 = max(data, getX)\n      if (x0 < min(data, getX)) x0 = min(data, getX)\n\n      let d = data.filter((row) => Math.round(getX(row)) === Math.round(x0))\n\n      if (d.length === 0) return null\n\n      const { y } = localPoint(event)\n\n      showTooltip({\n        tooltipData: d,\n        tooltipLeft: xScale(x0),\n        tooltipTop: y,\n      })\n    },\n    [data, showTooltip, xScale]\n  )\n\n  return (\n    <div className={className}>\n      <svg width={width} height={height}>\n        {dataGrouped.map((data, i) => (\n          <LinePath\n            key={i}\n            data={data.value}\n            x={(d) => xScale(getX(d))}\n            y={(d) => yScale(getY(d))}\n            stroke={colorScale(data.key)}\n          />\n        ))}\n        <g transform={`translate(${margin.left},0)`}>\n          <AxisLeft scale={yScale} />\n          <AnimatedGridRows\n            scale={yScale}\n            stroke={\"var(--color-paragraph)\"}\n            strokeWidth={0.2}\n            width={innerWidth}\n          />\n        </g>\n        <AxisBottom top={innerHeight + margin.top} scale={xScale} />\n        <Bar\n          x={margin.left}\n          y={margin.top}\n          width={innerWidth}\n          height={innerHeight}\n          fill='transparent'\n          rx={14}\n          onTouchStart={handleTooltip}\n          onTouchMove={handleTooltip}\n          onMouseMove={handleTooltip}\n          onMouseLeave={() => hideTooltip()}\n        />\n        {tooltipData && (\n          <g>\n            <Line\n              from={{ x: tooltipLeft, y: margin.top }}\n              to={{ x: tooltipLeft, y: innerHeight + margin.top }}\n              stroke='var(--color-paragraph)'\n              opacity={0.5}\n              strokeWidth={1}\n              pointerEvents='none'\n            />\n          </g>\n        )}\n      </svg>\n      {x && y && tooltipData && (\n        <TooltipWithBounds\n          key={Math.random()}\n          top={tooltipTop - 12}\n          left={tooltipLeft + 12}\n          style={tooltipStyles}\n        >\n          {tooltipData.length && (\n            <>\n              <h4 className='tooltip-title'>{getX(tooltipData[0])}</h4>\n              {tooltipData.map((d, i) => (\n                <div\n                  key={`tooltip-${i}`}\n                  style={{ display: \"flex\", justifyContent: \"space-between\" }}\n                >\n                  {color && (\n                    <p style={{ color: colorScale(getColor(d)) }}>\n                      {getColor(d)}\n                    </p>\n                  )}\n                  <p>{getY(d)}</p>\n                </div>\n              ))}\n            </>\n          )}\n        </TooltipWithBounds>\n      )}\n      {color !== \"none\" ? (\n        <Legend left={margin.left} scale={colorScale} />\n      ) : null}\n    </div>\n  )\n}\n\nconst ChartWrapper = ({ className }) => (\n  <ParentSize>\n    {({ width, height }) => (\n      <LineChart className={className} width={width} height={height} />\n    )}\n  </ParentSize>\n)\n\nexport default styled(ChartWrapper)`\n  .tooltip-title {\n    color: var(--color-background);\n    text-align: center;\n    font-weight: bold;\n  }\n`\n","/Users/owenking/web-development/data-viz-app/src/components/visualisations/visOptions.js",[],"/Users/owenking/web-development/data-viz-app/src/client.js",[],"/Users/owenking/web-development/data-viz-app/src/components/pages/index.js",[],"/Users/owenking/web-development/data-viz-app/src/components/pages/home.js",[],"/Users/owenking/web-development/data-viz-app/src/components/styled/elements/Control.js",[],"/Users/owenking/web-development/data-viz-app/src/components/styled/elements/ButtonOptions.js",[],"/Users/owenking/web-development/data-viz-app/src/components/App.js",[],"/Users/owenking/web-development/data-viz-app/src/components/GlobalStyles.js",[],"/Users/owenking/web-development/data-viz-app/src/components/styled/layout/Header.js",["287"],"import styled from \"styled-components\"\nimport { elevation } from \"../utilities\"\nimport Nav from \"./Nav\"\nimport { Link } from \"react-router-dom\"\nimport StyledLink from \"../elements/StyledLink\"\n\nconst Header = ({ className, openSignUp, openSignIn }) => (\n  <div className={className}>\n    <div>\n      <StyledLink className='logo' to='/'>\n        graphix\n      </StyledLink>\n    </div>\n    <Nav openSignUp={openSignUp} openSignIn={openSignIn} />\n  </div>\n)\n\nexport default styled(Header)`\n  grid-area: header;\n  display: flex;\n  place-items: center;\n  justify-content: space-between;\n  background: var(--color-foreground);\n  padding: 0 10px;\n  ${elevation[1]};\n\n  .logo {\n    text-decoration: none;\n    text-transform: uppercase;\n    font-size: 2rem;\n    color: var(--color-button);\n  }\n`\n","/Users/owenking/web-development/data-viz-app/src/components/styled/utilities/index.js",[],"/Users/owenking/web-development/data-viz-app/src/components/styled/utilities/Elevation.js",[],"/Users/owenking/web-development/data-viz-app/src/components/styled/elements/Panel.js",[],"/Users/owenking/web-development/data-viz-app/src/components/styled/layout/Footer.js",[],"/Users/owenking/web-development/data-viz-app/src/components/visualisations/AxisLeft.js",[],"/Users/owenking/web-development/data-viz-app/src/components/visualisations/AxisBottom.js",[],"/Users/owenking/web-development/data-viz-app/src/components/visualisations/Legend.js",[],"/Users/owenking/web-development/data-viz-app/src/components/styled/layout/Layout.js",[],"/Users/owenking/web-development/data-viz-app/src/hooks/useActive.js",[],"/Users/owenking/web-development/data-viz-app/src/components/styled/elements/Button.js",[],"/Users/owenking/web-development/data-viz-app/src/components/styled/elements/Select.js",[],"/Users/owenking/web-development/data-viz-app/src/components/styled/elements/NextSpan.js",[],["288","289"],"/Users/owenking/web-development/data-viz-app/src/components/styled/elements/Table.js",[],"/Users/owenking/web-development/data-viz-app/src/components/styled/utilities/themes.js",[],"/Users/owenking/web-development/data-viz-app/src/components/styled/utilities/palettes.js",[],"/Users/owenking/web-development/data-viz-app/src/components/styled/elements/Menu.js",[],"/Users/owenking/web-development/data-viz-app/src/components/pages/chartid.js",[],"/Users/owenking/web-development/data-viz-app/src/components/graphql/query.js",[],"/Users/owenking/web-development/data-viz-app/src/components/graphql/mutation.js",[],"/Users/owenking/web-development/data-viz-app/src/components/styled/layout/Nav.js",[],"/Users/owenking/web-development/data-viz-app/src/components/SignIn.js",[],"/Users/owenking/web-development/data-viz-app/src/components/SignUp.js",[],"/Users/owenking/web-development/data-viz-app/src/components/styled/elements/Modal.js",[],"/Users/owenking/web-development/data-viz-app/src/components/styled/elements/CloseButton.js",[],"/Users/owenking/web-development/data-viz-app/src/components/graphql/index.js",[],"/Users/owenking/web-development/data-viz-app/src/components/styled/elements/Input.js",[],"/Users/owenking/web-development/data-viz-app/src/components/styled/layout/form/Form.js",["290","291","292"],"import { useState } from \"react\"\nimport Geometry from \"./Geometry\"\nimport Share from \"./Share\"\nimport { useSelection } from \"../../../../hooks\"\nimport styled from \"styled-components\"\nimport Data from \"./Data\"\nimport Style from \"./Style\"\nimport { elevation } from \"../../utilities\"\nimport { useQuery } from \"@apollo/client\"\nimport { GET_DATA } from \"../../../graphql/query\"\n\nconst Form = ({ className }) => {\n  const { updateSelections } = useSelection()\n  const [id, setId] = useState()\n  const [open, setOpen] = useState(\"data\")\n\n  const { data, error, loading } = useQuery(GET_DATA, {\n    variables: { id },\n    onCompleted: (data) => {\n      updateSelections({\n        data: JSON.parse(data.getDataset.data),\n      })\n    },\n  })\n\n  return (\n    <div className={className}>\n      <Data open={open === \"data\"} setOpen={setOpen} handleChange={setId} />\n      <Geometry open={open === \"viz\"} setOpen={setOpen} />\n      <Style open={open === \"style\"} setOpen={setOpen} />\n      <Share open={open === \"share\"} setOpen={setOpen} />\n    </div>\n  )\n}\n\nexport default styled(Form)`\n  display: flex;\n  flex-direction: column;\n  align-content: stretch;\n  min-width: 300px;\n  ${elevation[1]};\n  overflow: hidden;\n\n  background: var(--color-foreground);\n\n  @media only screen and (max-width: 1024px) {\n    height: auto;\n  }\n`\n","/Users/owenking/web-development/data-viz-app/src/components/styled/layout/visualisation/Visualisation.js",[],"/Users/owenking/web-development/data-viz-app/src/components/styled/layout/form/Data.js",[],"/Users/owenking/web-development/data-viz-app/src/components/styled/layout/form/Style.js",[],"/Users/owenking/web-development/data-viz-app/src/components/styled/layout/form/Geometry.js",[],"/Users/owenking/web-development/data-viz-app/src/components/styled/layout/form/Share.js",[],"/Users/owenking/web-development/data-viz-app/src/components/styled/layout/form/VisOptions.js",[],"/Users/owenking/web-development/data-viz-app/src/components/styled/layout/form/GeoInput.js",[],"/Users/owenking/web-development/data-viz-app/src/components/styled/layout/form/ShareLink.js",[],"/Users/owenking/web-development/data-viz-app/src/components/styled/layout/form/AesInput.js"